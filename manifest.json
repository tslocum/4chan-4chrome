{
   "background_page": "background.html",
   "content_scripts": [ {
      "all_frames": true,
      "js": [ "4chan.js" ],
      "matches": [ "http://*.4chan.org/*", "http://4chanarchive.org/brchive/*", "http://inb4.im/4ch/*" ],
      "run_at": "document_end"
   } ],
   "default_locale": "en",
   "description": "__MSG_chrome_extension_description__",
   "icons": {
      "128": "images/4chan_icon.png"
   },
   "name": "4chan 4chrome",
   "options_page": "options.html",
   "page_action": {
      "default_icon": "images/4chan_icon_action.png",
      "default_title": "4chan 4chrome",
      "popup": "popup.html"
   },
   "permissions": [ "tabs", "http://www.4chan.org/" ],
   "version": "9001.48"
}
